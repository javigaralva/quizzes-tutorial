{
  "algorithm": {
    "parameters": [
      {
        "name": "id",
        "type": "integer"
      },
      {
        "name": "log",
        "type": "LogType"
      }
    ],
    "statements": [
      {
        "command": "Return",
        "expression": "RdsUpdate(Log.Log, ^log, id == ^id)"
      }
    ],
    "typeReturn": "any",
    "variables": []
  },
  "routing": {
    "method": "PATCH",
    "parameters": [
      {
        "bind": "id",
        "in": "Path",
        "name": "id",
        "remarks": "Id."
      },
      {
        "bind": "log",
        "in": "Body",
        "name": "body",
        "remarks": "New Log information to be updated."
      }
    ],
    "path": "/log/{id}",
    "remarks": "Allows to update an existing Log.",
    "responses": []
  }
}